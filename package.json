{
  "name": "teaching-studio",
  "version": "0.1.0",
  "private": true,
  "main": "dist-electron/main/index.js",
  "scripts": {
  "dev": "concurrently -k \"npm:dev:electron\" \"npm:dev:vite\" \"npm:dev:launch\"",
  "dev:vite": "vite",
  "dev:electron": "tsc -p tsconfig.electron.json --watch",
  "dev:launch": "wait-on tcp:5173 && wait-on \"dist-electron/main/index.js\" && electron .",

  "build": "npm run build:electron && vite build",
  "build:electron": "tsc -p tsconfig.electron.json",

  "lint": "eslint . --ext .ts,.tsx",
  "preview": "vite preview"
},
  "dependencies": {
    "archiver": "^7.0.1",
    "electron": "^40.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "wait-on": "^8.0.1"
  }
}
